{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@open-ui-kit/core",
  "version": "0.1.0",
  "description": "A comprehensive React component library built on Material-UI",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outshift-open/open-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/outshift-open/open-ui-kit/issues"
  },
  "homepage": "https://github.com/outshift-open/open-ui-kit/tree/main/packages/open-ui-kit#readme",
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "material-ui",
    "typescript",
    "open-source"
  ],
  "files": [
    "src/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./*.css": "./dist/*.css",
    ".": {
      "node": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "default": "./dist/index.umd.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "yarn lib:dev",
    "bundle": "rollup -c --bundleConfigAsCjs",
    "build": "yarn lib:build",
    "lib:build": "yarn workspace scripts build",
    "dev": "yarn lib:dev",
    "lib:dev": "yarn bundle --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "test": "jest --config=jest.config.js --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "NODE_OPTIONS=--max-old-space-size=4096 storybook build"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=7",
    "@mui/lab": ">=7",
    "@mui/material": ">=7",
    "@mui/x-date-pickers": "^7.26.0",
    "copy-to-clipboard": ">=3",
    "date-fns": ">=2",
    "dayjs": ">=1.11",
    "framer-motion": ">=12",
    "react": ">=18",
    "react-code-blocks": ">=0.1.6",
    "react-dom": ">=18",
    "react-router-dom": ">=7.5.0",
    "react-syntax-highlighter": ">=15.6",
    "react-virtuoso": ">=4",
    "sonner": ">=2",
    "use-debounce": ">=10",
    "zustand": ">=5"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
